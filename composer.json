  "keywords": ["freeipa", "rh idm"],
  "homepage": "https://github.com/staempfli/freeipa",
  "type": "library",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Marcel Hauri",
      "email": "marcel.hauri@staempfli.com"
    }
  ],
